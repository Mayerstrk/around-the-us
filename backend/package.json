	"description": "",
	"main": "./src/app.ts",
	"scripts": {
		"dev": "npx nodemon -r tsconfig-paths/register -x ts-node ./src/app.ts",
		"start": "npx ts-node -r tsconfig-paths/register ./src/app.ts",
		"test": "xo"
	},
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/bcrypt": "^5.0.0",
		"@types/cookie-parser": "^1.4.4",
		"@types/dotenv": "^8.2.0",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^9.0.3",
		"@types/mongoose": "^5.11.97",
		"@types/validator": "^13.11.1",
		"eslint-config-prettier": "^9.0.0",
		"eslint-config-xo": "^0.43.1",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"nodemon": "^2.0.21",
		"prettier": "^3.0.3",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.2.2",
		"xo": "^0.56.0"
	},
	"dependencies": {
		"@types/node": "^18.15.3",
		"ajv": "^6.12.6",
		"bcrypt": "^5.1.1",
		"cookie-parser": "^1.4.6",
		"dotenv": "^16.3.1",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.2",
		"mongoose": "^7.1.0",
		"validator": "^13.11.0"
	},
	"prettier": {
		"bracketSpacing": true
	},
	"xo": {
		"parser": "@typescript-eslint/parser",
		"plugins": [
			"@typescript-eslint"
		],
		"extends": [
			"plugin:@typescript-eslint/recommended"
		],
		"prettier": true,
		"rules": {
			"import/extensions": "off",
			"@typescript-eslint/naming-convention": "off",
			"@typescript-eslint/object-curly-spacing": [
				"error",
				"always"
			],
			"@typescript-eslint/consistent-type-definitions": [
				"error",
				"interface"
			]
		}
	}
